{
  "name": "mes-app-template",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run clean:dist && npm run build:module",
    "build:module": "tsc -b && cross-env VITE_BASE_PATH=/template VITE_APP_TYPE=template vite build",
    "build:spa": "tsc -b && cross-env VITE_BASE_PATH=/template-spa VITE_APP_TYPE=spa vite build",
    "preview": "tsc -b && cross-env VITE_BASE_PATH=/template-preview VITE_APP_TYPE=spa vite build --mode=preview && cross-env VITE_BASE_PATH=/template-preview vite preview",
    "clean:dist": "node scripts/clean-dist.js",
    "lint": "eslint ."
  },
  "dependencies": {
    "@radix-ui/react-label": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.16",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "i18next": "^25.6.2",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.3.3",
    "react-router-dom": "^7.9.5",
    "sonner": "^2.0.7",
    "tw-animate-css": "^1.4.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "cross-env": "^10.1.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}

