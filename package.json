{
  "name": "mes-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run clean:dist && npm-run-all --parallel build:*",
    "build:spa": "tsc -b && cross-env VITE_BASE_PATH=/ams-spa VITE_APP_TYPE=spa vite build",
    "build:example": "tsc -b && cross-env VITE_BASE_PATH=/example VITE_APP_TYPE=example vite build",
    "clean:dist": "node scripts/clean-dist.js",
    "lint": "eslint .",
    "preview": "tsc -b && cross-env VITE_BASE_PATH=/ams-spa-preview VITE_APP_TYPE=spa vite build --mode=preview && cross-env VITE_BASE_PATH=/ams-spa-preview vite preview"
  },
